{
    "name": "reactium",
    "version": "2.4.5",
    "description": "A framework for creating React + Redux apps using the domain driven design (DDD) paradigm.",
    "main": "index.js",
    "scripts": {
        "start": "node build/.core/index.js",
        "test": "jest",
        "manifest": "node ./.core/createManifest.js",
        "build": "npm-run-all build:*",
        "build:gulp": "cross-env NODE_ENV=production gulp",
        "build:babel-core": "cross-env NODE_ENV=production babel .core --out-dir build/.core",
        "build:babel-src": "cross-env NODE_ENV=production babel src --out-dir build/src",
        "build:cleanup": "npm prune --production",
        "static": "npm-run-all build:* && gulp static",
        "library": "gulp library",
        "local": "gulp local",
        "local:ssr": "gulp local:ssr",
        "clean": "gulp clean",
        "docker": "npm-run-all -s clean docker:*",
        "docker:build": "cross-env docker image build -t $npm_package_name:$npm_package_version -t $npm_package_name:latest .",
        "docker:push": "cross-env docker push $npm_package_name:$npm_package_version && cross-env docker push $npm_package_name:$npm_package_version:latest",
        "deploy": "cf push",
        "precommit": "lint-staged",
        "postinstall": "npm run autoinstall",
        "autoinstall": "run-script-os",
        "autoinstall:darwin:linux": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build; fi",
        "autoinstall:win32": ""
    },
    "keywords": [
        "react",
        "redux",
        "framework",
        "javascript",
        "ddd"
    ],
    "author": "Cam Tullos <cam@tullos.ninja> (http://cam.tullos.ninja) and John Dillick <john@dillick.us>",
    "license": "MIT",
    "engines": {
        "node": "9.2.1",
        "npm": ">=5.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Atomic-Reactor/Reactium.git"
    },
    "browser": {
        "parse/node": false
    },
    "dependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/node": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "ajv": "^6.5.2",
        "axios": "^0.18.0",
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.3",
        "cookie-session": "^2.0.0-beta.3",
        "copy-to-clipboard": "^3.0.8",
        "core-js": "^2.5.7",
        "cors": "^2.8.4",
        "cross-env": "^5.2.0",
        "directory-tree": "^2.0.0",
        "eslint": "^5.12.0",
        "express": "^4.16.2",
        "globby": "^8.0.1",
        "gsap": "^2.0.2",
        "htmltojsx": "^0.3.0",
        "http-auth": "^3.2.3",
        "http-proxy-middleware": "^0.19.1",
        "js-beautify": "^1.7.5",
        "marked": "^0.4.0",
        "moment": "^2.20.1",
        "morgan": "^1.9.1",
        "npm-run-all": "^4.1.5",
        "object-path": "^0.11.4",
        "parse": "^1.11.0",
        "prettier": "^1.15.1",
        "prop-types": "^15.6.2",
        "querystring-browser": "^1.0.4",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-frame-component": "^4.0.1",
        "react-helmet": "^5.2.0",
        "react-redux": "^6.0.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "react-syntax-highlighter": "^8.0.1",
        "redbox-react": "^1.6.0",
        "redux": "^4.0.0",
        "redux-devtools": "^3.5.0",
        "redux-devtools-dock-monitor": "^1.1.2",
        "redux-devtools-log-monitor": "^1.4.0",
        "redux-local-persist": "0.0.5",
        "redux-super-thunk": "^0.0.7",
        "run-script-os": "^1.0.5",
        "semver": "^5.5.0",
        "serialize-javascript": "^1.4.0",
        "underscore": "*",
        "xss": "^1.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "atomic-reactor-cli": "^2.0.21",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-module-resolver": "^3.1.1",
        "browser-sync": "^2.26.3",
        "camelcase": "^5.0.0",
        "chalk": "^2.3.0",
        "cli-spinners": "^1.3.1",
        "decamelize": "^2.0.0",
        "del": "^3.0.0",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "eslint-plugin-react": "^7.11.1",
        "fast-diff": "^1.2.0",
        "fs-extra": "^7.0.1",
        "fs-readdir-recursive": "^1.1.0",
        "gulp": "^4.0.0",
        "gulp-autoprefixer": "^5.0.0",
        "gulp-concat": "^2.6.1",
        "gulp-csso": "^3.0.1",
        "gulp-if": "^2.0.2",
        "gulp-install": "^1.1.0",
        "gulp-less": "^4.0.1",
        "gulp-rename": "^1.3.0",
        "gulp-run": "^1.7.1",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.4.1",
        "gulp-watch": "^5.0.0",
        "gulp4-run-sequence": "^0.2.1",
        "handlebars": "^4.0.12",
        "husky": "^0.14.3",
        "ignore-loader": "^0.1.2",
        "jest": "^23.6.0",
        "lint-staged": "^7.2.0",
        "module-alias": "^2.1.0",
        "node-sass": "^4.11.0",
        "node-sass-functions-json": "^1.0.0",
        "node-sass-tilde-importer": "^1.0.2",
        "nodemon": "^1.18.7",
        "ora": "^3.0.0",
        "readdir-recursive": "0.0.4",
        "slugify": "^1.3.3",
        "vinyl-source-stream": "^2.0.0",
        "webpack": "^4.16.1",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-filter-warnings-plugin": "^1.2.0",
        "webpack-hot-middleware": "^2.22.3",
        "yargs": "^12.0.1"
    },
    "libDependencies": [
        "react"
    ],
    "nodemonConfig": {
        "watch": [
            "src/manifest.js"
        ],
        "ignore": [
            "src/app/*"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
